{
	"manifest_version": 2,
	"name": "url-qrcode",
	"version": "1.0.0",
	"description": "pageAction演示",
	"icons":
	{
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"page_action":
	{
		"default_icon": "icon.png",
		"default_title": "我是pageAction",
		"default_popup": "popup.html"
	},
	"permissions":
	[
		"declarativeContent",
    "tabs",
    "contextMenus"
	],
	"background":
	{
    "page":"background.html"
	},
  "content_scripts": 
    [
        {
            "matches": ["<all_urls>"],
            "js": ["js/jquery-1.8.3.js", "js/content-script.js"],
            "css": ["css/custom.css"],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
       "main.js",
       "my-script.js"
    ]
}